{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src/scripts"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.m?js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./out"
  }
}
