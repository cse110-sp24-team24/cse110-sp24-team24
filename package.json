{
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@eslint/js": "^9.2.0",
    "babel-jest": "^29.7.0",
    "electron": "^30.0.6",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "jsdoc": "^4.0.3",
    "prettier": "^3.2.5",
    "puppeteer": "^22.8.0"
  },
  "scripts": {
    "start": "electron ./src/main.js",
    "lint-check": "npm run style:lint && npm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src/scripts"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./out",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "type": "module",
  "name": "cse110-sp24-team24",
  "description": "<hr style=\"border:2px solid gray\">",
  "version": "1.0.0",
  "main": "unitTest.js",
  "keywords": [],
